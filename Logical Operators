#include<stdio.h>
int main() { 
int a =15, b=10, c=5, d=19;
int e = !(a >= b && b>= c) || !( d!=a);
printf("%d \n", e);
return 0;
}

#include<stdio.h>
int main() { 
int a=14, b=10, c=5;
int d= 1 && 0;
int e = !( a+2<b || b<c);
printf("%d \n",e);
printf("%d \n", d);
return 0 ;
}



// logical operators
// logic gates : AND, OR, NOT
// Logical AND : && ,  Logical OR : ||, Logical  NOT : !
// E1 && E2 ==> a>b && b>c : If both E1 and E2 are TRUE --res--> TRUE
// E1 || E2 ==> If both E1 and E2 are FALSE --res--> FALSE
