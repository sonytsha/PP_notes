#include<stdio.h>
#include<math.h>
int main() {
int x = pow ( 4,3);
printf("%d\n", x);
return 0;
}


#include<stdio.h>
#include<math.h>
int main() {
long long x = pow ( 4,20);
printf("%lld\n", x);
return 0;
}

power 0.2 means square root

#include<stdio.h>
#include<math.h>
int main() {
long long x = pow ( 4,0.5);
printf("%lld\n", x);
return 0;
}

