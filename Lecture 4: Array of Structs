#include<stdio.h>
struct Person { 
char name[50];
int age;
char gender;
};


int main() {
int n,i;
scanf("%d", &n);
struct Person persons[n];
for(i=0;i<n;i++) {
scanf("%s %d %c", &(persons[i].name), &(persons[i].age), &(persons[i].gender));
}
printf("************\n");
for(i=0;i<n;i++) {
scanf("%s %d %c", (persons[i].name), (persons[i].age), (persons[i].gender));
}
return 0;
}


