// by different powers of n
n = 209106, n = n/10, n = 20910
n = 209106, n = n/100, n= 2091
n = 209106, n = n/1000, n= 209
n = 209106, n = n/10000, n= 20
n = 209106, n = n/100000, n= 2
n = 209106, n = n/1000000, n= 0

// generate that power of 10 whose division with n gives us single digit


#include<stdio.h>
int main() {
int N;
scanf("%d", &N);
int x=1;
while ( N/x > 9) {
x= x*10;
}
while( x > 0) {
printf("%d \n", N/x);
x /=10;
}
return 0;
}

