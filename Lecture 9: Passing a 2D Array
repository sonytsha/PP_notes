#include<stdio.h>

void func(int* a, int m, int n) {
int i;
for(i=0;i<m*n;i++) {
printf("%d", *(a+i));
printf("\n");
}

int main() {
int arr[3][2] = {{1,5},{2,4},{9,0}};
func(&arr[0][0],3,2);
return 0;
}


ORRRRR



#include<stdio.h>

void func( int m, int n, int mat[][n]) {
int i,j;
for(i=0;i<m;i++) {
for(j=0;j<n;j++) 
printf("%d", mat[i][j]);
}
printf("\n");
}

int main() {
int arr[3][2] = {{1,5},{2,4},{9,0}};
func(3,2, arr);
return 0;
}

