#include<stdio.h>
int main() {
char s[100] = "1278327342776327632";
long long x = 0;
int len =0, i =0;
while(s[i] !='\0') {
len++;
i++;
}
len--;
long long powerOfTen = 1;
while(len >=0) {
x +=(s[len] -'0' ) *powerOfTen;
len--;
powerOfTen *=10;
}
printf("%ld\n", x);
return 0;
}
