// print all natural numbers from 1 to 50- except the multiples of 7


#include<stdio.h>
int main() {
int i;
for ( i = 1; i<=50; i++) {
if ( i% 7 != 0) 
printf("%d", i);
}
return 0;
}


using continue : 

#include<stdio.h>
int main() {
int i;
for ( i = 1; i<=50; i++) {
if ( i% 7 == 0) 
continue;
printf("%d", i);
}
return 0;
}
