#include<stdio.h>
int main() {
int n;
int i;
scanf("%d", &n);
for( i = 1; i <= n; i++) {
printf("%d ", i);
}
i++;
printf("\n");
return 0;
}

ORR 


#include<stdio.h>
int main() {
int n ;
scanf("%d", &n);
int i = 1;
while ( i<= n) {
printf("%d ", i);
i++;
}
printf("\n");
return 0;
}

ORR  if OUTPUI : 1 -> 2 -> 3 -> 4 -> 5 -> 6


#include<stdio.h>
int main() {
int n ;
scanf("%d", &n);
int i = 1;
while ( i<= n) {
if ( i== n) {
printf("%d", i);
}
else {
printf("%d -> ", i);
}
i++;
}
printf("\n");
return 0;
}

OR 
#include<stdio.h>
int main() {
int n ;
scanf("%d", &n);
int i = 1;
while ( i<= n) {
if ( i== n) {
printf("%d", i++);
}
else {
printf("%d -> ", i++);
}
}
printf("\n");
return 0;
}



// 3 THINGS TO REMEMBER - 1) INITIALIZE LOOP CONTROL VARIABLE
2) CHANGE THE VALUE OF LOOP CONTROL VARIABLE ACCORDINGLY
3) LOOP CONDITION

2ND CODE - print N natural numbers and then print their sum in a new line

#include<stdio.h>
int main() {
int n ;
scanf("%d", &n);
int i = 1;
int sum  = 0;
while ( i<= n) {
printf("%d ", i);
sum = sum + i;
i++;
}
printf("\n");
printf("%d", sum);
return 0;
}

or SUM OF FIRST N NATURAL NUMBER FORMULA IS N*(N+1)/2

#include<stdio.h>
int main() {
int n ;
scanf("%d", &n);
int i = 1;
while ( i<= n) {
printf("%d ", i);
i++;
}
printf("\n");
printf("%d",(n*(n+1))/2);
return 0;
}


