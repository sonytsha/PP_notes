#include<stdio.h>
int main() {
int m,n;
printf("enter the numbers of rows and columns");
scanf("%d%d", &m,&n);
int nums1[m][n];
int sum[m][n];
int i,j;
printf("enter the  1st matrix");
for(i=0;i<m;i++) {
for (j=0;j<n;j++) {
scanf("%d", &nums1[i][j]);
}
}
int nums2[m][n];
printf("enter the  2nd matrix");
for(i=0;i<m;i++) {
for (j=0;j<n;j++) {
scanf("%d", &nums2[i][j]);
}
}
printf("the sum is");
for(i=0;i<m;i++) {
for (j=0;j<n;j++) {
sum[i][j] = nums1[i][j] + nums2[i][j];
printf("%d ", sum[i][j]);
}
printf("\n");
}
printf("\n");
return 0;
}
