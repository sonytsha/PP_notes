#include<stdio.h>
int main(){
int N;
scanf("%d", &N);
int i = 1;
while ( i<=N) {
int j = 1;
while(j <=i) {
int k = 1;
while ( k <= j) {
    printf("%d", k);
    k++;
}
printf("\n");
j++;
}
printf("xxxxxxxxxx\n");
i++;
}
return 0;
}



input : 3
output : 

1
xxxxxxxxxx
1
12
xxxxxxxxxx
1
12
123
xxxxxxxxxx

