floor : nearest interger which is smaller than or equeal

#include<stdio.h>
#include<math.h>
int main() {
int x = floor(5.9);
int y = floor( -5.9);
printf("%d\n", x);
printf("%d\n", y);
return 0;
}

ceil : nearest intetger which is greater than or equal

#include<stdio.h>
#include<math.h>
int main() {
int x = ceil(6.5);
int y = ceil(-8.9);
printf("%d%d",x,y);
}


sqrt : square root of a number

#include<stdio.h>
#include<math.h>
int main() {
int x = sqrt(49);
printf("%d\n", x);
}
