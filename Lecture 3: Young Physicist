#include<bits/stdc++.h>
using namespace std;

int main() {
int n;
cin >> n;
int sumx, sumy, sumz;
sumx = sumy = sumz = 0;
int x,y,z;
for(int i=0;i<n;i++) {
cin >>x >> y >>z;
sumx  += x;
sumy += y;
sumz += z;
}

if( sumx == 0 && sumy ==0 && sumz ==0 ) {
cout << "Yes"; }
else {
cout << "NO";
}

return 0;
}
